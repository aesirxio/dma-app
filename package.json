    "eslint": "^8.17",
    "microbundle": "^0.15.1",
    "postcss-scss": "^4.0.5",
    "prettier": "^2.8.4",
    "react-inject-env": "^2.1.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.52",
