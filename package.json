    "aesirx-dam-app": "aesirxio/dam-app",
    "aesirx-dma-lib": "aesirxio/dma-lib",
    "bootstrap": "5.1.3",
    "emoji-mart": "^3.0.0",
    "graphql": "^15.4.0",
    "html-react-parser": "^1.2.7",
    "i": "^0.3.6",
